AAAA;AAAA,uBAQA,KAAA,CAAA,GAAA,CAAA,sCAAA,CARA;;AAAA;AAQA,gBAAA,QARA;;AAAA;AAWA,gBAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,OAAA,CAAA,SAAA;AACA,kBAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,oBAAA,IAAA,EAAA,OAAA,CAAA,IADA;AAEA,oBAAA,MAAA,EAAA,OAAA,CAAA,MAFA;AAGA,oBAAA,SAAA,EAAA,OAAA,CAAA,SAHA;AAIA,oBAAA,SAAA,EAAA,MAAA,CAAA,OAAA,CAAA,SAAA,GAAA,GAAA,CAAA,CAAA,SAAA,CAAA,CAAA,EAAA,CAAA,IAAA;AAJA,mBAAA;AAMA,iBARA;;AAUA,oBAAA,QAAA,CAAA,KAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,kBAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,oBAAA,IAAA,EAAA,mBADA;AAEA,oBAAA,MAAA,EAAA,mBAFA;AAGA,oBAAA,SAAA,EAAA,KAHA;AAIA,oBAAA,SAAA,EAAA;AAJA,mBAAA;AAMA;;AA5BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OA5BA;AAAA;AAAA;;AA2DA,aAAA,UAAA,GAAA;AACA,MAAA,QAAA,CAAA,KAAA,GAAA,CAAA,QAAA,CAAA,KAAA;AAEA,MAAA,SAAA;AACA;;AAEA,IAAA,SAAA;AAEA,WAAA;AACA,MAAA,QAAA,EAAA,QADA;AAEA,MAAA,MAAA,EAAA,MAFA;AAGA,MAAA,QAAA,EAAA,QAHA;AAIA,MAAA,SAAA,EAAA,SAJA;AAKA,MAAA,UAAA,EAAA;AALA,KAAA;AAOA;AA5EA,CAAA","sourcesContent":["<template>\n  <div class=\"menu\">\n    <h2>Comments</h2>\n    <label class=\"refresh\">\n      <b-button variant=\"outline-primary\" @click=\"onRefresh\">Refresh</b-button>\n    </label>\n    <label class=\"filter-hate\">\n      <b-form-checkbox variant=\"outline-primary\" @change=\"filterHate\" aria-checked=\"false\">Only Hate Speech Filter</b-form-checkbox>\n    </label>\n    <div class=\"table\">\n      <b-table striped hover bordered :items=\"comments\" :fields=\"fields\">\n      </b-table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from '@vue/composition-api';\nimport axios from 'axios';\n\nexport default {\n  name: 'Comments',\n  setup() {\n    const comments = ref([]);\n    const fields = ref([]);\n    const hateOnly = ref(false);\n\n    fields.value = [\n      {\n        key: 'author',\n        label: 'Author',\n        sortable: true\n      },\n      {\n        key: 'subreddit',\n        label: 'Subreddit',\n        sortable: true\n      },\n      {\n        key: 'body',\n        label: 'Comment',\n        sortable: true\n      },\n      {\n        key: 'certainty',\n        label: 'Confidence',\n        sortable: true\n      }\n    ];\n\n    async function onRefresh() {\n\n      let response = [];\n      comments.value = [];\n\n      if (hateOnly.value) {\n        response = await axios.get('http://localhost:4000/getHateComments');\n      } else {\n        response = await axios.get('http://localhost:4000/getAllComments');\n      }\n\n      response.data.forEach(comment => {\n        console.log(comment.certainty);\n        comments.value.push({\n          body: comment.body,\n          author: comment.author,\n          subreddit: comment.subreddit,\n          certainty: String(comment.certainty * 100).substring(0, 6) + \" %\"\n        });\n      });\n\n      if (comments.value.length === 0) {\n        comments.value.push({\n          body: 'No comments found',\n          author: 'No comments found',\n          subreddit: 'N/A',\n          certainty: 'N/A'\n        })\n      }\n    }\n\n    function filterHate() {\n      hateOnly.value = !hateOnly.value;\n\n      onRefresh();\n    }\n\n    onRefresh();\n\n    return {\n      comments,\n      fields,\n      hateOnly,\n      onRefresh,\n      filterHate,\n    };\n  }\n}\n</script>\n\n<style scoped>\nh2 {\n  font-weight: bold;\n}\n\n.menu {\n  display: flex;\n  flex-direction: column;\n  padding: 50px;\n  align-items: center;\n  justify-content: center;\n}\n\n.table {\n  margin-top:0px;\n}\n\n.refresh {\n  float: right;\n  margin: 10px;\n  margin-right: 8px;\n  margin-left: 1220px;\n}\n\n.filter-hate {\n  float: right;\n  margin: 10px;\n  margin-top:0px;\n  margin-right: 20px;\n  margin-left: 1110px;\n}\n</style>"],"sourceRoot":"src/views"}]}