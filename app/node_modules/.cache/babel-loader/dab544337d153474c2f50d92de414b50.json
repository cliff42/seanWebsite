,uCAAA,CANA;;AAAA;AAMA,gBAAA,QANA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAQA,KAAA,CAAA,GAAA,CAAA,sCAAA,CARA;;AAAA;AAQA,gBAAA,QARA;;AAAA;AAWA,gBAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,OAAA,CAAA,SAAA;AACA,kBAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,oBAAA,IAAA,EAAA,OAAA,CAAA,IADA;AAEA,oBAAA,MAAA,EAAA,OAAA,CAAA,MAFA;AAGA,oBAAA,SAAA,EAAA,OAAA,CAAA,SAHA;AAIA,oBAAA,SAAA,EAAA,MAAA,CAAA,OAAA,CAAA,SAAA,GAAA,GAAA,CAAA,CAAA,SAAA,CAAA,CAAA,EAAA,CAAA,IAAA;AAJA,mBAAA;AAMA,iBARA;;AAUA,oBAAA,QAAA,CAAA,KAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,kBAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,oBAAA,IAAA,EAAA,mBADA;AAEA,oBAAA,MAAA,EAAA,mBAFA;AAGA,oBAAA,SAAA,EAAA,KAHA;AAIA,oBAAA,SAAA,EAAA;AAJA,mBAAA;AAMA;;AA5BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAlCA;AAAA;AAAA;;AAiEA,aAAA,UAAA,GAAA;AACA,MAAA,QAAA,CAAA,KAAA,GAAA,CAAA,QAAA,CAAA,KAAA;AAEA,MAAA,SAAA;AACA;;AAEA,IAAA,SAAA;AAEA,WAAA;AACA,MAAA,QAAA,EAAA,QADA;AAEA,MAAA,MAAA,EAAA,MAFA;AAGA,MAAA,QAAA,EAAA,QAHA;AAIA;AACA,MAAA,QAAA,EAAA,QALA;AAMA,MAAA,SAAA,EAAA,SANA;AAOA,MAAA,UAAA,EAAA;AAPA,KAAA;AASA;AApFA,CAAA","sourcesContent":["<template>\n  <div class=\"menu\">\n    <h2 class=\"title\">Films</h2>\n    <!-- <youtube :video-id=\"videoId0\" :player-vars=\"playerVars\" @playing=\"playing\"></youtube> -->\n    <youtube :video-id=\"videoId0\" @playing=\"playing\"></youtube>\n    <div class=\"g-ytsubscribe\" data-channelid=\"UCrhHo4x64Ong7FXdejiEf9Q\" data-layout=\"full\" data-theme=\"dark\" data-count=\"default\"></div>\n    <!-- <iframe src=\"http://www.youtube.com/embed/?listType=user_uploads&list=YOURCHANNELNAME\" width=\"480\" height=\"400\"></iframe> -->\n  </div>\n</template>\n\n<script>\nimport { ref } from '@vue/composition-api';\nimport axios from 'axios';\n\nexport default {\n  name: 'Comments',\n  setup() {\n    const comments = ref([]);\n    const fields = ref([]);\n    const hateOnly = ref(false);\n    const videoId0 = ref('n3pmrBaJSgA');\n    // const playerVars = ref({});\n\n    // playerVars = {\n    //     autoplay: 1\n    // }\n\n    fields.value = [\n      {\n        key: 'author',\n        label: 'Author',\n        sortable: true\n      },\n      {\n        key: 'subreddit',\n        label: 'Subreddit',\n        sortable: true\n      },\n      {\n        key: 'body',\n        label: 'Comment',\n        sortable: true\n      },\n      {\n        key: 'certainty',\n        label: 'Confidence',\n        sortable: true\n      }\n    ];\n\n    async function onRefresh() {\n\n      let response = [];\n      comments.value = [];\n\n      if (hateOnly.value) {\n        response = await axios.get('http://localhost:4000/getHateComments');\n      } else {\n        response = await axios.get('http://localhost:4000/getAllComments');\n      }\n\n      response.data.forEach(comment => {\n        console.log(comment.certainty);\n        comments.value.push({\n          body: comment.body,\n          author: comment.author,\n          subreddit: comment.subreddit,\n          certainty: String(comment.certainty * 100).substring(0, 6) + \" %\"\n        });\n      });\n\n      if (comments.value.length === 0) {\n        comments.value.push({\n          body: 'No comments found',\n          author: 'No comments found',\n          subreddit: 'N/A',\n          certainty: 'N/A'\n        })\n      }\n    }\n\n    function filterHate() {\n      hateOnly.value = !hateOnly.value;\n\n      onRefresh();\n    }\n\n    onRefresh();\n\n    return {\n      comments,\n      fields,\n      videoId0,\n      // playerVars,\n      hateOnly,\n      onRefresh,\n      filterHate,\n    };\n  }\n}\n</script>\n\n<style scoped>\nh2 {\n  font-weight: bold;\n}\n\n.menu {\n  display: flex;\n  flex-direction: column;\n  padding: 50px;\n  align-items: center;\n  justify-content: center;\n}\n\n.title {\n  padding: 50px;\n}\n</style>"],"sourceRoot":"src/views"}]}